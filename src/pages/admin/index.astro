---
export const prerender = false;
export function get() {
  const dest = '/admin/members';
  const body = `<!doctype html><html><head><meta http-equiv="refresh" content="0; url=${dest}"><meta name="robots" content="noindex"><title>Redirecting</title></head><body><p>Redirecting to <a href="${dest}">${dest}</a></p></body></html>`;
  return new Response(body, {
    status: 302,
    headers: {
      'Content-Type': 'text/html; charset=utf-8',
      Location: dest
    }
  });
}
---
