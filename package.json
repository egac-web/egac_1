{
  "name": "egac-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "test": "vitest",
    "check:js-no-ts": "node ./scripts/check-js-no-ts-annotations.js",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx,astro}\" --ext .js,.mjs,.cjs,.ts,.tsx,.jsx,.astro",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,astro,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,astro,json,md,css}\"",
    "lint:ci": "npm run check:js-no-ts && npm run lint -- --max-warnings=0 && npm run format:check"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/react": "^4.4.2",
    "@directus/sdk": "^20.2.0",
    "@supabase/supabase-js": "^2.89.0",
    "astro": "^5.16.6",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.561.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "vitest": "^4.0.16",
    "eslint": "^8.45.0",
    "prettier": "^2.8.8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.0"
  }
}
